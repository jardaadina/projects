<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.CompletareRaport">
    <children>
        <AnchorPane layoutY="-1.0" prefHeight="401.0" prefWidth="214.0" style="-fx-background-color: #498fba;">
            <children>
                <TextField fx:id="raport" layoutX="17.0" layoutY="57.0" prefHeight="45.0" prefWidth="176.0" promptText="id raport" />
                <TextField fx:id="programare" layoutX="17.0" layoutY="133.0" prefHeight="45.0" prefWidth="176.0" promptText="id programare" />
                <TextField fx:id="continut" layoutX="17.0" layoutY="262.0" prefHeight="45.0" prefWidth="176.0" promptText="continut" />
            <TextField fx:id="medic" layoutX="19.0" layoutY="201.0" prefHeight="45.0" prefWidth="176.0" promptText="id medic" />
            </children>
        </AnchorPane>
        <Button fx:id="inapoi" layoutX="286.0" layoutY="327.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="46.0" prefWidth="122.0" style="-fx-background-color: #498fba;" text="Inapoi" textFill="WHITE">
            <font>
                <Font name="Bookman Old Style Bold" size="18.0" />
            </font>
        </Button>
        <Button fx:id="adauga" layoutX="286.0" layoutY="267.0" mnemonicParsing="false" onAction="#adaugaOnAction" prefHeight="46.0" prefWidth="122.0" style="-fx-background-color: #498fba;" text="Adauga" textFill="WHITE">
            <font>
                <Font name="Bookman Old Style Bold" size="18.0" />
            </font>
        </Button>
      <Label fx:id="mesajFinalRaport" layoutX="250.0" layoutY="113.0" prefHeight="104.0" prefWidth="178.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Bookman Old Style Bold" size="16.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
